<?xml version="1.0"?>
<package>
  <name>rviz_satellite</name>
  <version>3.0.2</version>
  <description>Display satellite map tiles in RViz</description>
  <author email="gcross.code@icloud.com">Gareth Cross</author>
  <maintainer email="andre.schroeder@tomtom.com">Andre Schr√∂der</maintainer>
  <license>Apache 2</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <build_depend>qtbase5-dev</build_depend>
  <build_depend>rclcpp</build_depend>
  <build_depend>rviz_common</build_depend>
  <build_depend>rviz_default_plugins</build_depend>
  <build_depend>sensor_msgs</build_depend>

  <run_depend>rclcpp</run_depend>
  <run_depend>rviz_common</run_depend>
  <run_depend>rviz_default_plugins</run_depend>
  <run_depend>nav_msgs</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>tf2_ros</run_depend>

  <export>
      <rviz plugin="plugin_description.xml"/>
      <build_type>ament_cmake</build_type>
  </export>
</package>
